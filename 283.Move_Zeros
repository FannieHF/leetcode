class Solution(object):
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: void Do not return anything, modify nums in-place instead.
        """
        length = len(nums)
        pointer= 0
        while pointer < length and nums[pointer:] != [0]*(length-pointer):
            if nums[pointer] == 0:
                nums[pointer:] = nums[pointer+1:]+[0]
                pointer -= 1
            pointer += 1
